}

func Test_VisitComands(t *testing.T) {
	ri, err := NewRouteInfo()
	if err != nil {
		t.Fatalf("bad: %v", err)
	}

	var count int
	ri.VisitCommands(func(name string, cmd []string) {
		count++
	})
	if count == 0 {
		t.Fatalf("Expected more than 0 items")
	}
}

