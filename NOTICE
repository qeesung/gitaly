`,
			want: "Ethernet",
		},
	}

	for _, tc := range testCases {
		t.Run(tc.name, func(t *testing.T) {
			got, err := parseDefaultIfNameWindows(tc.routeOut, tc.ipconfigOut)
			if err != nil {
				t.Fatalf("unable to parse default interface from route output: %v", err)
